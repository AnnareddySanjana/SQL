CREATE database MyCompany;
drop database MyCompany;
use MyCompany;
CREATE TABLE employees(
	EmployeeId INT PRIMARY KEY,
    EmployeeName VARCHAR(50),
    EmployeeDept VARCHAR(60),
    salary DOUBLE NOT NULL
);
